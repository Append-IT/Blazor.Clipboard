<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<RootNamespace>Append.Blazor.Clipboard</RootNamespace>
		<AssemblyName>Append.Blazor.Clipboard</AssemblyName>
		<Authors>Benjamin Vertonghen</Authors>
		<Company>Append</Company>
		<Description>Clipboard API for Microsoft Blazor</Description>
		<Copyright>Benjamin Vertonghen</Copyright>
		<PackageProjectUrl>https://github.com/Append-IT/Blazor.Clipboard</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Append-IT/Blazor.Clipboard</RepositoryUrl>
		<PackageTags>Blazor;Clipboard;.NET;Razor;HTML</PackageTags>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.8" />
	</ItemGroup>
	<ItemGroup>
		<None Include="..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
</Project>
